################################################################################
## RealSearch properties
################################################################################

#Search Engine Configurations
realsearch.elasticsearch.server.host=rfint-search-es-srv1.altidev.net
realsearch.elasticsearch.server.port=9300
realsearch.elasticsearch.cluster.name=rf-realsearch
realsearch.solr.connectionString=http://rfint-search-es-srv1.altidev.net:8983/solr

#Rabbit MQ Configuration
realsearch.indexservice.amqp.host=rfint-search-rabbit-srv1.altidev.net
realsearch.indexservice.amqp.port=5672
realsearch.indexservice.amqp.username=realsearch
realsearch.indexservice.amqp.password=realsearch12
realsearch.indexservice.amqp.vhost=realsearch
realsearch.indexservice.amqp.insert.request=realsearch.data.insert.queue
realsearch.indexservice.amqp.update.request=realsearch.data.update.queue
realsearch.indexservice.amqp.delete.request=realsearch.data.delete.queue
realsearch.indexservice.amqp.audit.request=realsearch.data.audit.queue

#Multiple consumers configuration
index.concurrent.consumers=20
index.thread.pool.size=20
index.prefetch.count=20
audit.concurrent.consumers=10
audit.thread.pool.size=10
audit.prefetch.count=20

realsearch.indexservice.action.insert=realsearch.data.insert
realsearch.indexservice.action.delete=realsearch.data.delete
realsearch.indexservice.action.update=realsearch.data.update
realsearch.indexservice.action.audit=realsearch.data.audit

realsearch.indexservice.update.retries=4

documentation.services.basePath=http://rfint-search-tc-srv1.altidev.net:8080/searchservice/
documentation.services.version=1.0
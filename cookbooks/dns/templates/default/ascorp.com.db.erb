;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.ascorp.com. root.ascorp.com. (
                     <%= @serial %>     ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
		NS      ns
		NS	ns2
		NS	ns3
ns	A	<%= @dnsmaster %>
ns2	A	10.0.0.27
ns3	A	10.0.1.111
<% @ascorp.each do |ascorp| -%>
<%= ascorp %>
<% end -%>

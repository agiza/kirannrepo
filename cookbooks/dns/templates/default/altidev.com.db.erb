;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.altidev.com. root.altidev.com. (
                        <%= @serial %>  ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
		NS      ns
		NS	ns2
		NS	ns3
<% @altidev.each do |altidev| -%>
<%= altidev %>
<% end -%>
*	A	10.0.0.20

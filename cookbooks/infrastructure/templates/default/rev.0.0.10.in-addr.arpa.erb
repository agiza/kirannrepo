;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.altidev.com. root.altidev.com. (
                     <%= @serial %>     ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.
<% @rev0010.each do |rev| -%>
<%= "#{rev[:ipaddress]}".gsub!("10.0.0","") %>    IN    PTR     <%= rev[:fqdn] %>.
<% end -%>
<% @rev0010ex.each do |revex| -%>
<%= revex %>
<% end -%>
;*	IN	PTR	host.altidev.com.

amqp.host=<%= node[:amqp_host] %>
amqp.port=<%= node[:amqp_port] %>
amqp.username=<%= node[:rt_amqp_user] %>
amqp.password=<%= node[:rt_amqp_pass] %>
amqp.virtual-host=<%= node[:rt_amqp_vhost] %>

amqp.realservicing.order.create=realservicing.order.create
amqp.realservicing.order.response=realservicing.order.response
amqp.rt.core.deadmessage=rt.core.deadmessage

amqp.exchange.messages.inbound=rt.core.incoming
amqp.exchange.monitoring=rt.monitoring

realservicing.createOrdersRequestUrl=<%= realServicingRequestURL %>
realservicing.createOrdersResponseUrl=<%= realServicingResponseURL %>

monitoring.vendorOutboundChannel=createOrderRequestHttpChannel,createOrderResponseHttpChannel
monitoring.realtransInboundChannel=amqpCreateOrdersRequestChannel
monitoring.realtransOutboundChannel=amqpResponseChannel
monitoring.vendorInboundChannel=

monitoring.realtrans.inbound.source=Trans-Central
monitoring.realtrans.inbound.target=Int-RealServicing

monitoring.vendor.outbound.source=Int-RealServicing
monitoring.vendor.outbound.target=RealServicing

monitoring.vendor.inbound.source=
monitoring.vendor.inbound.target=

monitoring.realtrans.outbound.source=Trans-Central
monitoring.realtrans.outbound.target=Int-RealServicing

monitoring.taskExecutor.poolSize=10

rs.username=<%= rsUsername %>
rs.password=<%= rsPassword %>

poller.retry=4
poller.poolSize=10

schema.path=/ava/schemas/
json.schema=realServicing.json
header.schema=realServicingHeader.json


{
    "auditing_template" : {
        "template" : "*audit*",
		"settings" : {
			"analysis" : {
				"analyzer" : {
					"default" : {"type" : "keyword"}
				}
			}
		},		
        "mappings" : {			
            "_default_" : {
				"_all" : {"enabled" : false},
				"properties" : {
					"uid":{
						"type":"string"
					 },
					 "name":{
						"type":"string"
					 },
					 "request":{
						"properties":{
							"receivedTimeStamp" : {
								"type" : "date"
							},
							"requestType":{
								"type": "string"
							},
							"requestParameters":{
								"type": "object"
							},
							"urlParameters":{
								"type": "object"
							},
                            "requestBody":{
                                "type": "object"
                            },
							"requestPath":{
								"type": "object",
								"properties":{
									"requestUrl":{
										"type": "string"
									}																	
								}
							}
						}
					 },
					 "response":{
						"properties":{
							"sentTimeStamp" : {
								"type" : "date"
							},
							"responseStatus":{
								"type": "string"
							},
                            "responseBody":{
                                "type": "object"
                            }
						}
					 },
					 "host":{
						"type": "object"
					 }
				}
			} 
		}		
    }
}
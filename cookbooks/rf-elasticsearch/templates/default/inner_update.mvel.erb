List pathList = params.get('fieldsPath');
List values = params.get('values');
for (int i = 0; i < pathList.size(); i++) {
	fieldPath = pathList.get(i);
	value = values.get(i);
	pathSize = fieldPath.size();
	if (pathSize == 1) {
		ctx._source[fieldPath.get(0)] = value;
	} else if (pathSize == 2) {
		ctx._source[fieldPath.get(0)][fieldPath.get(1)] = value;
	} else if (pathSize == 3) {
		ctx._source[fieldPath.get(0)][fieldPath.get(1)][fieldPath.get(2)] = value;
	} else if (pathSize == 4) {
		ctx._source[fieldPath.get(0)][fieldPath.get(1)][fieldPath.get(2)][fieldPath.get(3)] = value;
	} else if (pathSize == 5) {
		ctx._source[fieldPath.get(0)][fieldPath.get(1)][fieldPath.get(2)][fieldPath.get(3)][fieldPath.get(4)] = value;
	} else if (pathSize == 6) {
		ctx._source[pathList.get(0)][pathList.get(1)][pathList.get(2)][pathList.get(3)][pathList.get(4)][pathList.get(5)] = value;
	} else if (pathSize == 7) {
		ctx._source[pathList.get(0)][pathList.get(1)][pathList.get(2)][pathList.get(3)][pathList.get(4)][pathList.get(5)][pathList.get(6)] = value;
	}
}
# DB configuration
db.url=<%= @accounts['db.url'] %>
db.username=<%= @accounts['db.username'] %>
db.password=<%= @accounts['db.password'] %>
db.showsql=<%= @accounts['db.showsql'] %>
db.driver=<%= @accounts['db.driver'] %>

############### Idology configuration ############
idology.expectedid.url=<%= @accounts['idology.expectedid.url'] %>
idology.expectedid.dq.url=<%= @accounts['idology.expectedid.dq.url'] %>
idology.expectedid.iq.url=<%= @accounts['idology.expectedid.iq.url'] %>
idology.user=<%= @accounts['idology.user'] %>
idology.pwd=<%= @accounts['idology.pwd'] %>

############### PROXY configuration!!! ############
## true/false --> proxy settings impact on sending the network request.
## IF env has restriction of using external resources, then we have to set up proxy settings.
proxy.on=<%= @accounts['proxy.on'] %>
proxy.host=<%= @accounts['proxy.host'] %>
proxy.port=<%= @accounts['proxy.port'] %>
proxy.user=<%= @accounts['proxy.user'] %>
proxy.pwd=<%= @accounts['proxy.pwd'] %>

# ESignature configuration
esignature.endpoint.url=<%= @backoffice['esignature.endpoint.url'] %>
esignature.username=<%= @backoffice['esignature.username'] %>
esignature.password=<%= @backoffice['esignature.password'] %>
esignature.integrator.key=<%= @backoffice['esignature.integrator.key'] %>
esignature.submitter.role=<%= @backoffice['esignature.submitter.role'] %>
esignature.listing.addendum.template.id=<%= @backoffice['esignature.listing.addendum.template.id'] %>
esignature.psa.contact.information.form.template.id=<%= @backoffice['esignature.psa.contact.information.form.template.id'] %>
esignature.http.proxyHost=<%= @backoffice['esignature.http.proxyHost'] %>
esignature.http.proxyPort=<%= @backoffice['esignature.http.proxyPort'] %>
esignature.https.proxyHost=<%= @backoffice['esignature.https.proxyHost'] %>
esignature.https.proxyPort=<%= @backoffice['esignature.https.proxyPort'] %>


esignature.assetmanager.role=Asset Manager
esignature.listingagent.role=Listing Agent
esignature.contact.information.carbon.copy.role=XDocumentProcessor

# document service configuration
document.service.baseurl=<%= @backoffice['document.service.baseurl'] %>
document.service.username=<%= @backoffice['document.service.username'] %>

# realdoc document IDs
psa.document.lbp.pamphlet.id=<%= @backoffice['psa.document.lbp.pamphlet.id'] %>
la.document.vacant.listingguide.id=<%= @backoffice['la.document.vacant.listingguide.id'] %>
la.document.occupied.listingguide.id=<%= @backoffice['la.document.occupied.listingguide.id'] %>

# Offer Processing email group
ash.offer.processing.emailAddress=<%= @backoffice['ash.offer.processing.emailAddress'] %>

#Envelop expiration settings
docusign.envelope.expire.enabled=true
docusign.envelope.expire.after.days=10
docusign.envelope.expire.warn.days=1
docusign.envelope.reminder.enabled=true
docusign.envelope.reminder.delay.days=1
docusign.envelope.reminder.frequency.days=1
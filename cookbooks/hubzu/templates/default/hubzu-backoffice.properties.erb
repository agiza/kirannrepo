# DB configuration
db.url=jdbc:mysql://<%= node[:db_server] %>:<%= node[:db_port] %>/<%= @mysqldb['hubzu_dbname'] %>?autoReconnect=true
db.username=<%= @mysqldb['hubzu_dbuser'] %>
db.password=<%= @mysqldb['hubzu_dbpass'] %>
db.showsql=true
db.driver=com.mysql.jdbc.Driver
db.validationQuery=Select 1
db.testOnBorrow=true
db.testWhileIdle=true
db.timeBetweenEvictionRunsMillis=10000

# JMS configurations
jms.amqp.endpoint=<%= @amqphost %>
jms.amqp.endport=<%= @amqpport %>
jms.amqp.username=<%= @amqpuser %>
jms.amqp.password=<%= @amqppass %>
jms.amqp.virtual-host=<%= node[:hubzu_amqp_vhost] %>
jms.exchange.name=backoffice.exchange

#Listing Agreement JMS configuration
jms.la.queue.name=property.la.add
jms.la.queue.routing.key=backoffice.la.add

#PSA JMS Configuration
jms.psa.queue.name=property.psa.add
jms.psa.queue.routing.key=backoffice.psa.add

#Listing Change JMS configuration
jms.la.change.queue.name=property.la.change.add
jms.la.change.queue.routing.key=backoffice.la.change.add

# Application configuration


# 1 minute delay
docusign.scheduler.delay=<%= @backoffice['docusign.scheduler.delay'] %>

#GFD transaction JMS Configuration
jms.gfd.transaction.queue.name=property.gfd.transaction
jms.gfd.transaction.queue.routing.key=backoffice.gfd.transaction.add

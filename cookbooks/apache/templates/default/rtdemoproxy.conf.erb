<Proxy balancer://rtdemo>
  Order allow,deny
  Allow from 10.
  Allow from 184.72.127.192
  Allow from 65.214.21.1/24
  Allow from 111.93.137.1/26
  Allow from 127.0.0.0/255.0.0.0 ::1/128
  BalancerMember http://realtrans-demo:8080 route=realtrans-demo
#  BalancerMember http://realtrans-qa:8080 route=realtrans-qa
  </Proxy>
<Location /realtrans-rp>
  <LimitExcept POST GET>
    Order allow,deny
    Deny from all
  </LimitExcept>

ProxyPass               balancer://rtdemo/realtrans-rp stickysession=JSESSIONID
#ProxyPassReverse        balancer://rtdemo/realtrans-rp stickysession=JSESSIONID
</Location>
<Location /realtrans-fp>
  <LimitExcept POST GET>
    Order allow,deny
    Deny from all
  </LimitExcept>
ProxyPass               balancer://rtdemo/realtrans-fp stickysession=JSESSIONID
#ProxyPassReverse       balancer://rtdemo/realtrans-fp stickysession=JSESSIONID

</Location>


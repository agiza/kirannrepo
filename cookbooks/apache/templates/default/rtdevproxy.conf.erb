<Proxy balancer://rtdev-cen>
    Order allow,deny
    #Allow from all
    Allow from 10.
    Allow from 184.72.127.192
    Allow from 65.214.21.1/24
    Allow from 111.93.137.1/26
    Allow from 127.0.0.0/255.0.0.0 ::1/128
    BalancerMember http://realtrans-intdev:8080 route=realtrans-intdev
    #BalancerMember http://realtrans-demo:8080 route=realtrans-demo
    ProxySet stickysession=JSESSIONID
</Proxy>

<Proxy balancer://rtdev-ven>
    Order allow,deny
    #Allow from all
    Allow from 10.
    Allow from 184.72.127.192
    Allow from 65.214.21.1/24
    Allow from 111.93.137.1/26
    Allow from 127.0.0.0/255.0.0.0 ::1/128
    BalancerMember http://realtrans-intdev2:8080 route=realtrans-intdev2
    #BalancerMember http://realtrans-demo:8080 route=realtrans-demo
    ProxySet stickysession=JSESSIONID
</Proxy>

ProxyRequests             Off
ProxyPreserveHost         On

<Location /realtrans-rp>
<LimitExcept POST GET>
  Order allow,deny
  Deny from all
</LimitExcept>

# RP
ProxyPass               balancer://rtdev-cen/realtrans-rp stickysession=JSESSIONID|jsessionid
#ProxyPassReverse        /realtrans-rp   balancer://rtqa-rp/realtrans-rp stickysession=JSESSIONID|jsessionid
</Location>

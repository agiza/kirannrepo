db.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
db.driver=com.mysql.jdbc.Driver
db.url=jdbc:mysql://<%= db_server %>:<%= db_port %>/<%= rt_dbname %>?autoReconnect=true
db.name=<%= rt_dbname %>
db.username=<%= db_user %>
db.password=<%= db_pass %>
db.initialSize=<%= db_initsize %>
db.maxActive=<%= db_maxactive %>
db.maxIdle=<%= db_maxidle %>
db.maxWait=<%= db_maxwait %>
db.removeAbandoned=true
db.removeAbandonedTimeout=600
db.timeBetweenEvictionRunsMillis=<%= db_timeevict %>
db.validationQuery=SELECT 1
db.validationQueryTimeout=<%= db_valquerytimeout %>
amqp.host=<%= amqp_host %>
amqp.port=<%= amqp_port %>
amqp.username=<%= amqp_user %>
amqp.password=<%= amqp_pass %>
ldap.port=<%= ldap_port %>
ldap.url=ldap://<%= ldap_server %>:<%= ldap_port %>
documentService.baseDirectoryPath=/opt/tomcat/storage/docs


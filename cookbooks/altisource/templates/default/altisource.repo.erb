[altisourcecommon]
name=Packages from Altisource - noarch
baseurl=http://<%= node[:yum_server] %>/yum/common
enabled=1
gpgcheck=0
proxy=_none_

[altisourcetesting]
name=Packages from Altisource - testing
baseurl=http://<%= node[:yum_server] %>/yum/testing
enabled=<%= node[:yum_test_setting] %>
gpgcheck=0
proxy=_none_

[altisourcerelease]
name=Packages from Altisource - release
baseurl=http://<%= node[:yum_server] %>/yum/release
enabled=<%= node[:yum_release_setting] %>
gpgcheck=0
proxy=_none_


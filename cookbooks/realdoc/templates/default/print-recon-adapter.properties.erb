rd.adapter.print.vendorCode=<%= @vendor_code %>
rd.adapter.print.receiptFilenameExpression=.*_RCPT_.*
rd.adapter.print.reconPollingInterval=10000
rd.adapter.print.signalFileExtension=.cmp
rd.adapter.print.reconFileExtension=.zip
rd.adapter.print.manifestFileExtension=.ack
rd.adapter.print.receiptManifestFileExtension=.rcpt
rd.adapter.print.errorFileExtension=.err

rd.adapter.print.inputDir=<%= @dirs[:input] %>
rd.adapter.print.signatures.inputDir=<%= @dirs[:input] %>
rd.adapter.print.signatures.errorFilePath=<%= @dirs[:input] %>

rd.adapter.print.workDir=<%= @dirs[:working] %>
rd.adapter.print.signatures.workDir=<%= @dirs[:working] %>

rd.adapter.print.signatures.archiveDir=<%= @dirs[:signature_archive] %>
rd.adapter.print.archiveDir=<%= @dirs[:ack_archive] %>
rd.adapter.print.errorFilePath=<%= @dirs[:error_archive] %>

rd.adapter.mongo.host=<%= @mongo[:host] %>
rd.adapter.mongo.db=<%= @mongo[:database] %>

rd.adapter.amqp.host=<%= @amqp[:host] %>
rd.adapter.amqp.port=<%= @amqp[:port] %>
rd.adapter.amqp.username=<%= @amqp[:username] %>
rd.adapter.amqp.password=<%= @amqp[:password] %>
rd.adapter.amqp.vhost=<%= @amqp[:vhost] %>

rd.adapter.amqp.inboundExchange=rd.responses
rd.adapter.amqp.inboundRoutingKey=correspondence.recon
rd.adapter.amqp.signature.importQueue=rd.correspondence.recon
rd.adapter.amqp.signature.inboundRoutingKey=correspondence.signature
rd.adapter.amqp.signature.inboundExchange=rd.responses

rd.adapter.print.sftp.host=<%= @ftp[:host] %>
rd.adapter.print.sftp.user=<%= @ftp[:username] %>
rd.adapter.print.sftp.pword=<%= @ftp[:password] %>
rd.adapter.print.sftpDir=<%= @ftp[:ack_dir] %>
rd.adapter.print.signatures.sftpDir=<%= @ftp[:signature_dir] %>
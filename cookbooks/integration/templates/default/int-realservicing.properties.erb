rt.int.amqp.realservicing.order.create=realservicing.order.create
rt.int.amqp.realservicing.order.response=realservicing.order.response
rt.int.amqp.rt.core.deadmessage=rt.core.deadmessage

rt.int.amqp.host=<%= @amqphost %>
rt.int.amqp.port=<%= @amqpport %>
rt.int.amqp.username=<%= @amqpuser %>
rt.int.amqp.password=<%= @amqppass %>
rt.int.amqp.virtual-host=<%= node[:rt_amqp_vhost] %>

rt.int.amqp.exchange.messages.inbound=rt.core.incoming
rt.int.amqp.exchange.monitoring=rt.monitoring

<% if @realsvc["requesturl#{node.chef_environment}"].nil?  || @realsvc["requesturl#{node.chef_environment}"].empty? -%>
rt.int.realservicing.retrieveOrdersRequestUrl=<%= @realsvc["requesturl"] %>
rt.int.realservicing.createOrderResponseUrl=<%= @realsvc["responseurl"] %>
<% else -%>
rt.int.realservicing.retrieveOrdersRequestUrl=<%= @realsvc["requesturl#{node.chef_environment}"] %>
rt.int.realservicing.createOrderResponseUrl=<%= @realsvc["responseurl#{node.chef_environment}"] %>
<% end -%>

rt.int.monitoring.realtransInboundChannel=newOrderRequestChannel,cancelOrderRequestChannel,modifyOrderRequestChannel
rt.int.monitoring.vendorOutboundChannel=newOrderRequestHttpChannel,cancelOrderRequestHttpChannel,modifyOrderRequestHttpChannel
rt.int.monitoring.vendorInboundChannel=newOrderAckChannel,incomingOrderResponseChannel,cancelOrderAckChannel,modifyRouterResponseChannel
rt.int.monitoring.realtransOutboundChannel=realtransOrderResponseChannel

rt.int.monitoring.realtrans.inbound.source=RealServicing
rt.int.monitoring.realtrans.inbound.target=Int-RealServicing

rt.int.monitoring.vendor.outbound.source=Int-RealServicing
rt.int.monitoring.vendor.outbound.target=Trans-Central

rt.int.monitoring.vendor.inbound.source=Trans-Central
rt.int.monitoring.vendor.inbound.target=Int-RealServicing

rt.int.monitoring.realtrans.outbound.source=Int-RealServicing
rt.int.monitoring.realtrans.outbound.target=RealServicing

rt.int.monitoring.taskExecutor.poolSize=10

rt.int.header.source.name=Int-RealServicing
rt.int.header.target.name=RealServicing

rt.int.rs.username=<%= @realsvc['username'] %>
rt.int.rs.password=<%= @realsvc['password'] %>

rt.int.poller.retry=4
rt.int.poller.poolSize=10
rt.int.realservicing.poller.delay=<%= @realsvc['rspollerdelay'] %>

rt.int.schema.path=/ava/schemas/
rt.int.json.schema=realServicing.json
rt.int.header.schema=realServicingHeader.json


rres.ftp.host=<%= @realres['ftphost'].split(":")[0] %>
rres.ftp.port=<%= @realres['ftphost'].split(":")[1] %>
rres.ftp.user=<%= @realres['user'] %>
rres.ftp.pwd=<%= @realres['pass'] %>
rres.filename.pattern=*.xml
rres.remote.in.directory=/<%= node[:chef_environment] %>/input
rres.local.directory=/tmp
rres.ftp.poller.delay=120000

rres.remote.archive.directory=/<%= node[:chef_environment] %>/archive
rres.remote.error.directory=/<%= node[:chef_environment] %>/error

monitoring.taskExecutor.poolSize=10

amqp.username=<%= @amqpuser %>
amqp.password=<%= @amqppass %>
amqp.host=<%= @amqphost %>
amqp.port=<%= @amqpport %>
amqp.virtual-host=<%= node[:rt_amqp_vhost] %>

amqp.exchange.messages.inbound=rt.core.incoming
amqp.queue.inbound=realresolution.order.response
amqp.rt.core.deadmessage=rt.core.deadmessage
amqp.exchange.monitoring=rt.monitoring

monitoring.vendorOutboundChannel=
monitoring.realtransInboundChannel=amqpCreateOrdersRequestChannel
monitoring.realtransOutboundChannel=avaOrderResponseChannel
monitoring.vendorInboundChannel=

monitoring.realtrans.inbound.source=Trans-Central
monitoring.realtrans.inbound.target=Int-RealResolution

monitoring.vendor.outbound.source=Int-RealResolution
monitoring.vendor.outbound.target=RealResolution

#No communication from RealServicing to our integration
monitoring.vendor.inbound.source=RealResolution
monitoring.vendor.inbound.target=Int-RealResolution

monitoring.realtrans.outbound.source=Trans-Central
monitoring.realtrans.outbound.target=Int-RealResolution


rt.int.monitoring.taskExecutor.poolSize=10
rt.int.schema.path=/ava/schemas/
rt.int.json.schema=realServicing.json
rt.int.header.schema=realServicingHeader.json

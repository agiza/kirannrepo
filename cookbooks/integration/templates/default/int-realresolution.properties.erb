rt.int.rres.ftp.host=<%= @realres['ftphost'].split(":")[0] %>
rt.int.rres.ftp.port=<%= @realres['ftphost'].split(":")[1] %>
rt.int.rres.ftp.user=<%= @realres['user'] %>
rt.int.rres.ftp.pwd=<%= @realres['pass'] %>
rt.int.rres.filename.pattern=*.xml
<% if @realres['ftpindir'].nil? || @realres['ftpindir'].empty? -%>
rt.int.rres.remote.in.directory=/<%= "#{node.chef_environment}" %>/input
<% else -%>
rt.int.rres.remote.in.directory=<%= @realres['ftpindir'] %>
<% end -%>
<% if @realres['ftpoutdir'].nil? || @realres['ftpoutdir'].empty? -%>
rt.int.rres.remote.out.directory=/<%= "#{node.chef_environment}" %>/out
<% else -%>
rt.int.rres.remote.out.directory=<%= @realres['ftpoutdir'] %>
<% end -%>
<% if @realres['ftparchivedir'].nil? || @realres['ftparchivedir'].empty? -%>
rt.int.rres.remote.archive.directory=/<%= "#{node.chef_environment}" %>/archive
<% else -%>
rt.int.rres.remote.archive.directory=<%= "#{@realres['ftparchivedir']}" %>
<% end -%>
<% if @realres['ftperrordir'].nil? || @realres['ftperrordir'].empty? -%>
rt.int.rres.remote.error.directory=/<%= "#{node.chef_environment}" %>/error
<% else -%>
rt.int.rres.remote.error.directory=<%= "#{@realres['ftperrordir']}" %>
<% end -%>

rt.int.amqp.username=<%= @amqpuser %>
rt.int.amqp.password=<%= @amqppass %>
rt.int.amqp.host=<%= @amqphost %>
rt.int.amqp.port=<%= @amqpport %>
rt.int.amqp.heartbeat=60
rt.int.amqp.virtual-host=<%= node[:realtrans_amqp_vhost] %>

rt.int.amqp.exchange.messages.inbound=rt.core.incoming
rt.int.amqp.queue.inbound=realresolution.order.response
rt.int.amqp.rt.core.deadmessage=rt.core.deadmessage
rt.int.amqp.exchange.monitoring=rt.monitoring

rt.int.monitoring.taskExecutor.poolSize=10

rt.int.rres.local.directory=${catalina.base}/CMA/input
rt.int.rres.poller.delay=<%= @realsvc['rspollerdelay'] %>

rt.int.rres.logback.maxHistory=<%= @maxhistory %>
rt.int.rres.logback.maxFileSize=<%= @maxfilesize %>
